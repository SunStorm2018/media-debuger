<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
"http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.media.mediadebuger6">
    <method name="GetBasicInfo">
      <arg name="info_type" type="s" direction="in"/>
      <arg name="result" type="s" direction="out"/>
    </method>
    <method name="GetMediaInfo">
      <arg name="file_path" type="s" direction="in"/>
      <arg name="stream_type" type="s" direction="in"/>
      <arg name="frame_type" type="s" direction="in"/>
      <arg name="result" type="s" direction="out"/>
    </method>
    <method name="OpenMediaInfoWindow">
      <arg name="file_path" type="s" direction="in"/>
      <arg name="stream_type" type="s" direction="in"/>
      <arg name="frame_type" type="s" direction="in"/>
      <arg name="success" type="b" direction="out"/>
    </method>
    <method name="IsAvailable">
      <arg name="available" type="b" direction="out"/>
    </method>
    <method name="GetVersion">
      <arg name="version" type="s" direction="out"/>
    </method>
  </interface>
</node>